{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/build-blog.mjs && tsx scripts/prerender-pages.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:visual": "playwright test --project=visual",
    "test:visual:update": "playwright test --project=visual --update-snapshots",
    "test:visual:ui": "playwright test --project=visual --ui",
    "test:explore": "tsx tests/agentic/exploratory-runner.ts",
    "test:gen": "tsx tests/testgen/generate.ts",
    "test:gen:run": "npm run test:gen && playwright test tests/testgen/generated",
    "build:ci": "npm ci && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "job-radar:ingest": "tsx scripts/job-radar-ingest/index.ts"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "lucide-react": "^0.542.0",
    "marked": "^16.2.1",
    "mermaid": "^11.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
