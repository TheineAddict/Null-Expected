{
  "trailingSlash": false,
  "cleanUrls": true,
  "redirects": [
    { "source": "/:path*.html", "destination": "/:path*", "permanent": true },
    { "source": "/About", "destination": "/about", "permanent": true },
    { "source": "/Mission", "destination": "/mission", "permanent": true },
    { "source": "/Manifesto", "destination": "/manifesto", "permanent": true }
  ],
  "rewrites": [
    { "source": "/((?!.*\\.).*)", "destination": "/" }
  ]
}
